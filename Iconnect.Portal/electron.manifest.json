{
  "executable": "Iconnect.Portal",
  "splashscreen": {
    "imageFile": ""
  },
  "singleInstance": true,
  "build": {
    "appId": "com.Iconnect.Portal.app",
    "productName": "Iconnect.Portal",
    "copyright": "Copyright Â© 2023",
    "buildVersion": "2.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "win": {
      "icon": "Resources/images/icon.png"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}